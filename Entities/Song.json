{
  "name": "Song",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "description": "Song title"
    },
    "artist": {
      "type": "string",
      "description": "Artist name"
    },
    "album": {
      "type": "string",
      "description": "Album name"
    },
    "duration": {
      "type": "string",
      "description": "Song duration in MM:SS format"
    },
    "genre": {
      "type": "string",
      "description": "Music genre"
    },
    "year": {
      "type": "number",
      "description": "Release year"
    },
    "rating": {
      "type": "number",
      "description": "User rating out of 5"
    },
    "votes": {
      "type": "number",
      "default": 0,
      "description": "Number of votes"
    },
    "is_explicit": {
      "type": "boolean",
      "default": false,
      "description": "Contains explicit content"
    },
    "spotify_id": {
      "type": "string",
      "description": "Spotify track ID"
    },
    "cover_image": {
      "type": "string",
      "description": "Album cover image URL"
    },
    "audio_file": {
      "type": "string",
      "description": "Audio file URL"
    },
    "bpm": {
      "type": "number",
      "description": "Beats per minute"
    },
    "is_karaoke": {
      "type": "boolean",
      "default": false,
      "description": "Available for karaoke"
    }
  },
  "required": [
    "title",
    "artist"
  ]
}